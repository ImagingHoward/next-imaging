<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="nodejs" path="server.js" verb="*" modules="FastCgiModule" scriptProcessor="C:\Program Files\iisnode\iisnode.dll" resourceType="Unspecified" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="Next.js" stopProcessing="true">
          <match url=".*" />
          <action type="Rewrite" url="server.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
